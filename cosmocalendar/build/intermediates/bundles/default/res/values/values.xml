    <color name="default_weekend_day_text_color">#00cf78</color>
    <declare-styleable name="CalendarView"><attr format="enum" name="firstDayOfTheWeek">
            <enum name="sunday" value="1"/>
            <enum name="monday" value="2"/>
            <enum name="tuesday" value="3"/>
            <enum name="wednesday" value="4"/>
            <enum name="thursday" value="5"/>
            <enum name="friday" value="6"/>
            <enum name="saturday" value="7"/>
        </attr><attr format="enum" name="selectionType">
            <enum name="single" value="0"/>
            <enum name="multiple" value="1"/>
            <enum name="range" value="2"/>
            <enum name="none" value="3"/>
        </attr><attr format="enum" name="orientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr><attr format="color" name="calendarBackgroundColor"/><attr format="color" name="monthTextColor"/><attr format="color" name="otherDayTextColor"/><attr format="color" name="dayTextColor"/><attr format="color" name="weekDayTitleTextColor"/><attr format="color" name="disabledDayTextColor"/><attr format="color" name="selectionBarMonthTextColor"/><attr format="color" name="selectedDayTextColor"/><attr format="color" name="selectedDayBackgroundColor"/><attr format="color" name="selectedDayBackgroundStartColor"/><attr format="color" name="selectedDayBackgroundEndColor"/><attr format="color" name="currentDayTextColor"/><attr format="reference" name="currentDayIconRes"/><attr format="reference" name="currentDaySelectedIconRes"/><attr format="reference" name="connectedDayIconRes"/><attr format="reference" name="connectedDaySelectedIconRes"/><attr format="enum" name="connectedDayIconPosition">
            <enum name="top" value="0"/>
            <enum name="bottom" value="1"/>
        </attr><attr format="reference" name="previousMonthIconRes"/><attr format="reference" name="nextMonthIconRes"/><attr format="color" name="weekendDayTextColor"/><attr name="weekendDays">
            <flag name="none" value="0"/>
            <flag name="monday" value="1"/>
            <flag name="tuesday" value="2"/>
            <flag name="wednesday" value="4"/>
            <flag name="thursday" value="8"/>
            <flag name="friday" value="16"/>
            <flag name="saturday" value="32"/>
            <flag name="sunday" value="64"/>
        </attr></declare-styleable>
    <dimen name="day_of_week_padding_vertical">@dimen/space_1x</dimen>
    <dimen name="day_padding_vertical">@dimen/space_1x</dimen>
    <dimen name="day_text_padding">8dp</dimen>
    <dimen name="day_text_size">14sp</dimen>
    <dimen name="month_name_padding">@dimen/space_1x</dimen>
    <dimen name="space_1x">8dp</dimen>
    <dimen name="space_2x">16dp</dimen>
    <dimen name="space_3x">24dp</dimen>
    <dimen name="space_4x">32dp</dimen>
    <dimen name="space_5x">40dp</dimen>
    <dimen name="space_6x">48dp</dimen>
    <dimen name="space_7x">56dp</dimen>
    <dimen name="space_8x">64dp</dimen>
    <dimen name="text_big">16sp</dimen>
    <dimen name="text_large">18sp</dimen>
    <dimen name="text_medium">14sp</dimen>
    <dimen name="text_small">12sp</dimen>
    <string name="app_name">Customizable Calendar</string>
